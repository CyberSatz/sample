
    <script>
        var req = new XMLHttpRequest();
        req.onload = reqListener;
        req.open('get','https://jsonplaceholder.typicode.com/posts/1',true);
        req.withCredentials = true;
        req.send();
    
        function reqListener() {
            location='/satheeshm'+alert(this.responseText);
        };
    </script>

